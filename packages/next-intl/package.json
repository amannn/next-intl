{
  "name": "next-intl",
  "version": "2.0.5",
  "sideEffects": false,
  "author": "Jan Amann <jan@amann.me>",
  "description": "A minimal, but complete solution for managing translations, date and number formatting in Next.js apps.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amannn/next-intl"
  },
  "scripts": {
