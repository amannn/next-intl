{
  "name": "next-intl-extracted",
  "private": true,
  "version": "0.1.0",
  "description": "Rust-based tools for next-intl message extraction",
  "scripts": {
    "build:extractor": "napi build --platform --release --cargo-cwd crates/extractor --config napi.config.js dist/extractor",
    "build:swc-plugin": "cargo build --target wasm32-wasip1 --release --manifest-path crates/swc-plugin/Cargo.toml",
    "test:swc-plugin": "cargo test --manifest-path crates/swc-plugin/Cargo.toml",
    "test:extractor": "cargo test --manifest-path crates/extractor/Cargo.toml",
    "test:extractor:node": "node tests/extractor.js"
  },
  "keywords": [],
  "author": "Jan Amann <jan@amann.work>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amannn/next-intl.git"
  },
  "files": [],
  "napi": {
    "name": "next-intl-extractor",
    "manifestPath": "crates/extractor/Cargo.toml",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.16.0"
  }
}
